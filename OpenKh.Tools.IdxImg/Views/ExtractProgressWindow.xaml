<Window x:Class="OpenKh.Tools.IdxImg.Views.ExtractProgressWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:OpenKh.Tools.IdxImg.Views"
        mc:Ignorable="d"
        Title="Extracting" Height="80" Width="300" ResizeMode="NoResize" SizeToContent="Height" WindowStartupLocation="CenterScreen">
    <StackPanel Margin="10">
        <TextBlock Margin="0 0 0 3">
            <Run>Extracting </Run>
            <Run Text="{Binding FileName}" d:Text="myfilename.bin"/>
        </TextBlock>
        <ProgressBar Height="22" IsIndeterminate="True"/>
    </StackPanel>
</Window>
